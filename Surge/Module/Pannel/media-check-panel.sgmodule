#!name=Streaming & AIGC Service Monitor
#!desc=检测 Netflix、Disney+、YouTube Premium、Spotify、ChatGPT、Claude、Gemini API 的可用性
#!author=HotKids & ChatGPT
#!system=ios
#!arguments=geminiapikey:null
#!arguments-desc=geminiapikey: Google Gemini API Key（可选，填写后启用 Gemini API 检测）

[Panel]
media-check-panel = type=generic,script-name=media-check,update-interval=600,icon=play.circle.fill

[Script]
media-check = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/HotKids/Rules/master/Surge/Module/Scripts/media-check.js,argument=geminiapikey={{{geminiapikey}}}

